						<h3 class="resume-title">Certificates</h3>
						{{#each data.resume.certificates}}
							<div class="resume-item">
								{{#if this.course}}
									<h4>{{this.course}}</h4>
								{{/if}}
								{{#if this.duration}}
									<h5>{{this.duration}}</h5>
								{{/if}}
								{{#if this.institute}}
									<p><em>{{this.institute}}</em></p>
								{{/if}}
								{{#if this.desc}}
									<p>{{this.desc}}</p>
								{{/if}}
							</div><!-- Edn Resume Item -->
						{{/each}}
					</div>
				{{/if}}
				{{#if data.resume.education}}
