<main class="main">

	<!-- Hero Section -->
	<section id="hero" class="hero section dark-background">

		{{#if data.banner}}
		<img src="{{data.banner}}" alt="" data-aos="fade-in" class="" />
		{{/if}}
		<div class="container" data-aos="fade-up" data-aos-delay="100">
			{{#if data.name}}
			<h2>{{data.name}}</h2>
			{{/if}}
			{{#if data.roles}}
			<p>I'm
				<span
					class="typed"
					data-typed-items="{{data.roles}}"
				>{{roles}}</span><span
					class="typed-cursor typed-cursor--blink"
					aria-hidden="true"
				></span><span
					class="typed-cursor typed-cursor--blink"
					aria-hidden="true"
				></span></p>
			{{/if}}
		</div>

	</section><!-- /Hero Section -->

	{{#if data.about}}
	<!-- About Section -->
	<section id="about" class="about section">

		<!-- Section Title -->
		<div class="container section-title" data-aos="fade-up">
			<h2>About</h2>
			{{#if data.about.objective}}
			<p>{{data.about.objective}}</p>
			{{/if}}
		</div><!-- End Section Title -->

		<div class="container" data-aos="fade-up" data-aos-delay="100">

			<div class="row gy-4 justify-content-center">
				<div class="col-lg-4">
					{{#if data.about.profileImg}}
						<img src="{{data.about.profileImg}}" class="img-fluid" alt="" />
					{{/if}}
				</div>
				<div class="col-lg-8 content">
					{{#if data.about.currentRole}}
						<h2>{{data.about.currentRole}}</h2>
					{{/if}}
					{{#if data.about.personal}}
					<div class="row">
						<div class="col-lg-12">
							<ul class="abt_ul">
								{{#each data.about.personal}}
									<li><i class="bi bi-chevron-right"></i>
										<strong>{{this.name}}:</strong>
										{{#if this.url}}
										<span><a
												href="tel:{{this.url}}"
											>{{this.value}}</a><span>
											{{else}}
										<span>{{this.value}}</span>
										{{/if}}
									</li>
								{{/each}}
							</ul>
						</div>
						{{/if}}
						{{!-- <div class="col-lg-6">
							<ul>
								<li><i class="bi bi-chevron-right"></i>
									<strong>Age:</strong>
									<span>{{data.about.personal.age}}</span></li>
								<li><i class="bi bi-chevron-right"></i>
									<strong>Degree:</strong>
									<span>{{data.about.personal.degree}}</span></li>
								<li><i class="bi bi-chevron-right"></i>
									<strong>Email:</strong>
									<span><a
											href="mailto:{{data.about.personal.email}}"
										>{{data.about.personal.email}}</a></span></li>
							</ul>
						</div> --}}
					</div>
					{{#if data.about.desc}}
					<p class="py-3">
						{{data.about.desc}}
					</p>
					{{/if}}
				</div>
			</div>
		</div>
	</section><!-- /About Section -->
	{{/if}}

