	<!-- Services Section -->
	{{#if data.project}}
		<section id="services" class="services section">
			<!-- Section Title -->
			<div class="container section-title" data-aos="fade-up">
				<h2>Projects</h2>
				{{#if data.project.desc}}
					<p>{{data.project.desc}}</p>
				{{/if}}
			</div><!-- End Section Title -->

			{{#if data.project.data}}
				<div class="container">
					<div class="row gy-4">
						{{#each data.project.data}}
							<div
								class="col-lg-6 col-md-6 service-item d-flex"
								data-aos="fade-up"
								data-aos-delay="{{@index}}00"
							>
							{{#if this.class}}
								<div class="icon flex-shrink-0"><i
										class="bi bi-{{this.class}}"
									></i></div>
							{{/if}}
								<div>
									{{#if this.title}}
									<h4 class="title">{{this.title}}</a></h4>
									{{/if}}
									<p class="description">{{this.desc}}
									{{#if this.url}}
									<br/>
									<span class="description">url: <a
											href="{{this.url}}"
